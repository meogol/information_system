<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="3dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <padding
                android:bottom="30dp"
                android:left="30dp"
                android:right="30dp"
                android:top="30dp" />

            <size
                android:width="120dp"
                android:height="120dp" />
            <solid android:color="#FAFAFA" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</layer-list>